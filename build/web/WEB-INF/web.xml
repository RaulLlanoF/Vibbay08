<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InicioSesion</servlet-name>
        <servlet-class>Servlet.InicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <servlet-class>Servlet.RegistrarUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistarProductos</servlet-name>
        <servlet-class>Servlet.RegistarProductos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Busqueda</servlet-name>
        <servlet-class>Servlet.Busqueda</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarPuja</servlet-name>
        <servlet-class>Servlet.RegistrarPuja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarPuja</servlet-name>
        <servlet-class>Servlet.CerrarPuja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pruebasImagen</servlet-name>
        <servlet-class>Servlet.pruebasImagen</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InicioSesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <url-pattern>/registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistarProductos</servlet-name>
        <url-pattern>/registrarobjetos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Busqueda</servlet-name>
        <url-pattern>/buscar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarPuja</servlet-name>
        <url-pattern>/registrarpuja</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarPuja</servlet-name>
        <url-pattern>/cerrarpuja</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pruebasImagen</servlet-name>
        <url-pattern>/pruebasImagen</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
